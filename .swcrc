{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "target": "es2022",
    "loose": true,
    "externalHelpers": false
  },
  "module": {
    "type": "es6"
  },
  "minify": true,
  "sourceMaps": true
}
